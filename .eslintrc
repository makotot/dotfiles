{
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "globals": {
    "$": false,
    "jQuery": false,
    "_": false,
    "Backbone": false
  },
  "rules": {
    "quotes": [1, "single"],
    "strict": [0],
    "max-params": [1, 4],
    "semi": [1, "always"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}]
  }
}
